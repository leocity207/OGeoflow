  node [fontname="Helvetica", fontsize=10];
  edge [fontname="Helvetica", fontsize=9];

  /* HTML table showing node color mapping */
  legend_table [shape=none, margin=0, label=<
    <TABLE BORDER="0" CELLBORDER="1" CELLSPACING="4" CELLPADDING="6">
